{
  "name": "supporticon",
  "version": "10.0.1",
  "description": "A libary to handle fetching data from JustGiving",
  "main": "index.js",
  "scripts": {
    "start": "styleguidist server",
    "build": "babel source -d dist",
    "test:lint": "standard -d source",
    "test:unit": "mocha source/**/**/__tests__/*.js --require @babel/register --require test/common.js",
    "test": "npm run test:lint && npm run test:unit",
    "format": "prettier-standard 'source/**/*.js'",
    "precommit": "yarn lint-staged",
    "docsify:watch": "docsify serve ./source",
    "styleguide:build": "styleguidist build",
    "docs:build": "npm run styleguide:build && gulp docs-prepare",
    "docs:deploy": "gh-pages -d styleguide -t true",
    "deploy:prepare": "npm run build && gulp npm-prepare",
    "announce": "./bin/announce"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud-services/supporticon.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud-services/supporticon/issues"
  },
  "author": "Dan Henderson <danhenderson.web@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "constructicon": "^4.1.2",
    "css-loader": "^5.1.1",
    "docsify-cli": "^4.4.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "husky": "^7.0.2",
    "jsdom": "^17.0.0",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.1",
    "moxios": "^0.4.0",
    "prettier": "^2.4.0",
    "prettier-standard": "^16.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-styleguidist": "^11.1.7",
    "sinon": "^11.1.2",
    "source-map-loader": "^1.0.0",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "dayjs": "^1.10.6",
    "js-base64": "^3.7.0",
    "prop-types": "^15.7.2",
    "react-query": "^3.31.0",
    "slugify": "^1.6.0",
    "spark-md5": "^3.0.2",
    "url-parse": "^1.5.3"
  },
  "peerDependencies": {
    "constructicon": "^3.0.0 || ^4.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "ignore": [
      "**/__tests__/",
      "/test/",
      "/dist/",
      "styleguide.config.js",
      "gulpfile.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "lint-staged": {
    "source/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
