{
  "name": "supporticon",
  "version": "6.2.10",
  "description": "A libary to handle fetching data from Supporter",
  "main": "index.js",
  "scripts": {
    "start": "styleguidist server",
    "build": "babel source -d dist",
    "test:lint": "standard -d source",
    "test:unit": "mocha source/**/**/__tests__/*.js --compilers js:babel-register",
    "test": "npm run test:lint && npm run test:unit",
    "format": "prettier-standard 'source/**/*.js'",
    "precommit": "lint-staged",
    "book:prepare": "gitbook install",
    "book:watch": "npm run book:prepare && gitbook serve",
    "book:build": "npm run book:prepare && rm -rf styleguide/book && gitbook build . styleguide/book",
    "styleguide:build": "styleguidist build",
    "docs:build": "npm run styleguide:build && npm run book:build",
    "deploy:prepare": "npm run build && gulp npm-prepare",
    "docs:deploy": "gh-pages -d styleguide",
    "announce": "./bin/announce"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackbaud-services/supporticon.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud-services/supporticon/issues"
  },
  "author": "Dan Henderson <danhenderson.web@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "constructicon": "^3.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.6.0",
    "gh-pages": "^0.12.0",
    "gitbook-cli": "^2.3.0",
    "gulp": "^3.9.1",
    "husky": "^0.14.3",
    "jsdom": "^9.12.0",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.4",
    "minimal.css": "^1.0.1",
    "mocha": "^3.2.0",
    "moxios": "^0.4.0",
    "numbro": "^2.3.2",
    "prettier": "^1.14.3",
    "prettier-standard": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^6.2.6",
    "sinon": "^1.17.7",
    "standard": "^8.6.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dayjs": "^1.10.5",
    "js-base64": "^3.6.0",
    "prop-types": "^15.5.8",
    "slugify": "^1.4.0",
    "spark-md5": "^3.0.0",
    "url-parse": "^1.4.3"
  },
  "peerDependencies": {
    "constructicon": "^2.0.0 || ^3.0.0",
    "numbro": "^1.0.0 || ^2.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard": {
    "ignore": [
      "**/__tests__/",
      "/test/",
      "/dist/",
      "styleguide.config.js",
      "gulpfile.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  }
}
